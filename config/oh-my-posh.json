{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#21c7c7",
          "style": "plain",
          "template": "\u256d\u2500 ",
          "type": "text"
        },
        {
          "background": "p:dark_red",
          "foreground": "p:yellow",
          "leading_diamond": "\ue0ba<p:yellow,p:dark_red>\ue0ba\ue0bc\ue0bb\ue0ba\ue0bc</>",
          "properties": {
            "display_host": true
          },
          "style": "diamond",
          "template": " {{ if .SSHSession }} {{ end }}<b>{{ .UserName }}</b><i>@{{ .HostName }}</i> ",
          "type": "session"
        },
        {
          "background": "#31181E",
          "foreground": "#29FFFF",
          "powerline_symbol": "<p:yellow,p:dark_red>\ue0ba\ue0bc</>",
          "properties": {
            "home_icon": "~",
            "mapped_locations": {},
            "max_depth": 5,
            "style": "agnoster_full"
          },
          "style": "powerline",
          "template": " \udb80\ude4b {{ path .Path .Location }} ",
          "type": "path"
        },
        {
          "background": "p:yellow",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "root_icon": "\uf0ad"
          },
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "p:dark_red",
          "foreground": "p:blue",
          "foreground_templates": [
            "{{ if gt .Code 0}}p:red{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "templates": [
            "{{ if gt .Code 0 }}\ue0bb\ue0bb<p:red,p:dark_red>\ue0ba\ue0bc</>{{ end }}",
            "{{ if eq .Code 0 }}<p:yellow,p:dark_red>\ue0bb\ue0ba\ue0bc</>{{ end }}"
          ],
          "trailing_diamond": "\ue0bc",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "foreground": "#21C7C7",
          "template": "\u251C\u2500\u2500\u2500 ",
          "style": "plain",
          "type": "python"
        },
        {
          "foreground": "#FFE366",
          "background": "#0d487a",
          "leading_diamond": "<p:red,default>\ue0b3</>\ue0b2",
          "trailing_diamond": "\ue0b0<p:red,default>\ue0b1</>",
          "properties": {
            "fetch_virtual_env": true,
            "display_version": false
          },
          "style": "diamond",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}\ue73c {{ .Venv }} {{ end }}venv: {{ .Full }}{{ end }}",
          "type": "python"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "foreground": "#21C7C7",
          "template": "\u251C\u2500\u2500\u2500 ",
          "style": "plain",
          "type": "git"
        },
        {
          "background": "p:dark_green",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:dark_yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:dark_red{{ end }}",
            "{{ if gt .Ahead 0 }}p:black{{ end }}",
            "{{ if gt .Behind 0 }}p:dark_purple{{ end }}"
          ],
          "foreground": "p:green",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
            "{{ if gt .Ahead 0 }}p:white{{ end }}",
            "{{ if gt .Behind 0 }}p:purple{{ end }}"
          ],
          "leading_diamond": "<p:red,default>\ue0b3</>\ue0b2",
          "trailing_diamond": "\ue0b0<p:red,default>\ue0b1</>",
          "properties": {
            "branch_max_length": 60,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "\udb80\udea2"
          },
          "style": "diamond",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#21c7c7",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#e0f8ff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "> ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "ðŸ’» zsh",
  "final_space": true,
  "palette": {
    "black": "#0A0E21",
    "blue": "#3abff8",
    "green": "#00FF9C",
    "dark_green": "#00603A",
    "orange": "#FBBD23",
    "red": "#FF5C53",
    "dark_red": "#31181E",
    "white": "#E0DEF4",
    "yellow": "#FFEE00",
    "dark_yellow": "#423F00",
    "rust": "#831f08",
    "teal": "#4CE2B6",
    "maroon": "#922D50",
    "purple": "#C07EEC",
    "dark_purple": "#320B4C",
    "primary": "#00ff9c"
  },
  "secondary_prompt": {
    "background": "p:dark_red",
    "foreground": "p:yellow",
    "template": " <,p:yellow> </> > <p:dark_red,transparent> </>"
  },
  "tooltips": [
    {
      "background": "p:orange",
      "foreground": "p:white",
      "leading_diamond": "\ue0b6",
      "properties": {
        "display_default": true
      },
      "style": "diamond",
      "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
      "tips": [
        "aws"
      ],
      "trailing_diamond": "\ue0b4",
      "type": "aws"
    },
    {
      "background": "p:blue",
      "foreground": "p:white",
      "leading_diamond": "\ue0b6",
      "style": "diamond",
      "template": " \ufd03 {{ .Name }} ",
      "tips": [
        "az"
      ],
      "trailing_diamond": "\ue0b4",
      "type": "az"
    }
  ],
  "transient_prompt": {
    "background": "p:dark_red",
    "foreground": "p:yellow",
    "template": "\ue0ba\ue0bc\ue0ba\ue0bc {{ .UserName }}@{{ .HostName }} \ue0ba\ue0bc <p:blue> {{ .Folder }} </><p:dark_red,transparent>\ue0b0</><p:black,transparent> </>"
  },
  "version": 2
}